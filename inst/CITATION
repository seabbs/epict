citHeader("To cite epict in publications use:")

citEntry(entry = "Article",
  title        = "epict: Cycle threshold modelling", # nolint
  author       = personList(as.person("Sam Abbott"),
    as.person("Timothy Russell"), as.person("Joel Hellewell")),
  journal      = "Zenodo",
  year         = "2022",
  volume       = "",
  number       = "",
  pages        = "",
  doi          = "",

  textVersion  =
  paste("Sam Abbott, Timothy Russell, Joel Hellewell (2022).",
        "epict: Cycle threshold modelling, ", # nolint
        "DOI: ")
)